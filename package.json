{
	"name": "ddev",
	"version": "0.0.1",
	"description": "Simple, and fast template for DiscordJS using TypeScript and Bun.",
	"module": "src/index.ts",
	"type": "module",
	"scripts": {
		"check": "npx biome check --write",
		"build": "bun run check && bun run scripts/build.ts",
		"build:windows": "bun run build -- --windows",
		"build:linux": "bun run build -- --linux",
		"build:macos": "bun run build -- --macos",
		"build:all": "bun run build -- --all",
		"start": "bun run check && bun run src/index.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/bun": "latest",
		"pino-pretty": "^11.2.2"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"discord.js": "^14.15.3",
		"glob": "^11.0.0",
		"pino": "^9.3.2"
	},
	"trustedDependencies": ["@biomejs/biome"]
}
